START
PROGRAM testLogicalContinuation ;
BEGIN
  ITEM bool'1 B;

  bool'1 = TRUE OR FALSE;
  bool'1 = TRUE AND TRUE AND TRUE;
  bool'1 = (TRUE AND TRUE) AND TRUE;
  bool'1 = TRUE AND (TRUE AND TRUE);

  bool'1 = TRUE AND TRUE AND TRUE AND TRUE AND TRUE AND TRUE;
  bool'1 = TRUE OR TRUE OR TRUE OR FALSE OR TRUE OR FALSE OR FALSE OR TRUE OR FALSE OR TRUE;
END
TERM
